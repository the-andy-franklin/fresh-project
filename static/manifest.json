{
  "name": "Mezo",
  "description": "Mezo is a sample PWA",
  "id": "mezo",
  "short_name": "Mezo",
  "lang": "en",
  "dir": "auto",
  "categories": ["entertainment"],
  "theme_color": "#1F2937",
  "background_color": "#1F2937",
  "display": "standalone",
  "display_override": ["window-controls-overlay"],
  "appType": "pwa",
  "scope": "/",
  "start_url": "/",
  "orientation": "any",
  "offline": "/offline.html",
  "icons": [
    {
      "src": "/pwa_logo.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/pwa_logo.mask.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "not-preferred",
  "launch_handler": {
    "client_mode": "navigate-new"
  },
  "screenshots": [
    {
      "src": "/pwa_screenshot.png",
      "sizes": "1280x720",
      "type": "image/png"
    }
  ],
  "shortcuts": [{
    "name": "Home",
    "url": "/"
  }],
  "protocol_handlers": [
    {
      "protocol": "web+music",
      "url": "/play?track=%s"
    }
  ],
  "iarc_rating_id": "e58c174a-81d2-5c3c-32cc-34b8de4a52e9",
  "related_applications": [
    {
      "platform": "play",
      "id": "com.mezo.app"
    }
  ],
  "prefer_related_applications": false,
  "scope_extensions": [
    {
      "type": "web-bundle",
      "url": "/mezo.webbundle"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "widgets": [
    {
      "name": "Mezo",
      "url": "/widget",
      "features": {
        "resizeable": true,
        "scrolling": true,
        "frame": "chrome"
      }
    }
  ]
}
