{
  "$schema": "https://json.schemastore.org/web-manifest",
  "name": "Mezo",
  "description": "Mezo is a sample PWA",
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "browser"
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "not-preferred",
  "short_name": "Mezo",
  "theme_color": "#1F2937",
  "background_color": "#1F2937",
  "display": "standalone",
  "appType": "pwa",
  "scope": "/",
  "start_url": "/",
  "orientation": "any",
  "offline": "/offline.html",
  "icons": [
    {
      "src": "/pwa_logo.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/pwa_logo.mask.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ]
}
